<p id="notice"><%= notice %></p>

<h1>Scheduled Habits</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @scheduled_habits.each do |scheduled_habit| %>
      <tr>
        <td><%= link_to 'Show', scheduled_habit %></td>
        <td><%= link_to 'Edit', edit_scheduled_habit_path(scheduled_habit) %></td>
        <td><%= link_to 'Destroy', scheduled_habit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Scheduled Habit', new_scheduled_habit_path %>
